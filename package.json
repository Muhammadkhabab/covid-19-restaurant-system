{
  "name": "covid-19-restaurant-system",
  "version": "1.0.0",
  "description": "CS 506 Group Project, Madison Small Restaurant Covid-19 Info System",
  "main": "server.js",
  "scripts": {
    "test": "cross-env NODE_ENV=test mocha ./test --recursive --exit --timeout 50000",
    "start": "node server",
    "server": "nodemon server",
    "client": "npm start --prefix client",
    "dev": "concurrently \"npm run server\" \"npm run client\"",
    "heroku-postbuild": "NPM_CONFIG_PRODUCTION=false npm install --prefix client && npm run build --prefix client",
    "seeds": "node seeds",
    "coverage": "nyc --reporter=html --reporter=text npm run test",
    "frontend-test": "selenium-side-runner ./test/client/*.side"
  },
  "author": "Prasoon Sinha",
  "license": "MIT",
  "dependencies": {
    "bcryptjs": "^2.4.3",
    "cross-env": "^7.0.3",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "express-validator": "^6.10.0",
    "jsonwebtoken": "^8.5.1",
    "moment": "^2.29.1",
    "mongoose": "^5.11.19",
    "node-geocoder": "^3.27.0",
    "node-schedule": "^2.0.0",
    "nodemailer": "^6.5.0",
    "nodemailer-express-handlebars": "^4.0.0",
    "path": "^0.12.7"
  },
  "devDependencies": {
    "chai": "^4.3.3",
    "chromedriver": "^89.0.0",
    "concurrently": "^6.0.0",
    "faker": "^5.4.0",
    "mocha": "^8.3.1",
    "nodemon": "^2.0.7",
    "nyc": "^15.1.0",
    "supertest": "^6.1.3",
    "supertest-prefix": "^1.0.2"
  },
  "nyc": {
    "exclude": [
      "services",
      "test",
      "seeds",
      "config"
    ]
  }
}